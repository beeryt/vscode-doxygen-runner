{
	"name": "doxygen-rnuner",
	"displayName": "doxygen-runner",
	"publisher": "betwo",
	"license": "MIT",
	"description": "Support for generating and previewing doxygen documentation",
	"version": "1.0.0",
	"icon": "assets/icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/betwo/vscode-doxygen-runner"
	},
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"taskDefinitions": [
			{
				"type": "doxygen_generate",
				"properties": {
					"task": {
						"type": "string",
						"description": "Generate doxygen documentation"
					}
				}
			}
		],
		"problemMatchers": [
			{
				"name": "doxygen-runner",
				"fileLocation": "absolute",
				"owner": "doxygen-runner",
				"pattern": [
					{
						"regexp": "^(.*):(\\d+):\\s+(warning|error):\\s+(.*)[^:]$",
						"file": 1,
						"line": 2,
						"severity": 3,
						"message": 4
					}
				]
			},
			{
				"name": "doxygen-runner-multiline",
				"fileLocation": "absolute",
				"owner": "doxygen-runner",
				"pattern": [
					{
						"regexp": "^(.*):(\\d+):\\s+(warning|error):\\s+(.*):$",
						"file": 1,
						"line": 2,
						"severity": 3,
						"message": 4
					},
					{
						"regexp": "^(\\s+[^\\s].*)$",
						"message": 1,
						"loop": true
					}
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"vscode": "^1.1.34",
		"typescript": "^3.3.3333"
	},
	"dependencies": {},
	"extensionDependencies": []
}